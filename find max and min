#include <iostream>
#include<limits.h>

using namespace std;

int main()
{   
    int a[]={21,34,32,2,12,6,7,8,5,4,3,2,13,25,9,27,24,};
    int size=17;
    int MAX=INT_MIN;
    int MIN=INT_MAX;
    
    for(int  i=0;i<size;i++){
        if(a[i]>MAX){
            
            MAX=a[i];
        }
        if(a[i]<MIN){
            MIN=a[i];
           
        }
        
    }
    cout<<"maximum number is\n"<<MAX;
    cout<<"\nminimum number is \n"<<MIN;
 }
