#include <iostream>
#include<limits.h>

using namespace std;

int main()
{   
    int a[]={21,34,32,2,12,6,7,8,5,4,3,2,13,25,9,27,24};
    int size=17;
    int start=0;
    int end=size-1;
    
   while(start<=end){
       swap(a[start],a[end]);
       start++;
       end--;
   }
   for(int i=0;i<size;i++){
       cout<<a[i]<<" ";
   }
}
