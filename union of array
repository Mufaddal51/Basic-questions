#include <iostream>
#include<vector>

using namespace std;


int main()
{
    
 int arr[]={1,2,4,3,1,7};
  int sizea=6;
  int brr[]={5,4,6,7};
  int sizeb=4;
  vector<int>ans;
  for(int i=0;i<sizea;i++){
      ans.push_back(arr[i]);
      
  }
  for(int i=0;i<sizeb;i++){
      ans.push_back(brr[i]);
  }
  for(int i=0;i<ans.size();i++){
      cout<<ans[i];
  }
     
}
