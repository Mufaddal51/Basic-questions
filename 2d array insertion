#include <iostream>

using namespace std;

int main()
{
   int arr[4][4];
   
   int row=4;
   
   int col=4;
   
        for(int i=0;i<row;i++){
            
        for(int j=0;j<col;j++){
            
           cin>>arr[i][j];
           
           }
        }
        cout<<"printing row wise\n";
        for(int i=0;i<row;i++){
            
        for(int j=0;j<col;j++){
            
           cout<<arr[i][j]<<" ";
           
           }cout<<endl;
       
   }

}
