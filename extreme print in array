#include <iostream>
#include<limits.h>

using namespace std;

int main()
{   
    int a[]={21,34,32,2,12,6,7,8,5,4,3,2,13,25,9,27,24};
    int size=17;
    int start=0;
    int end=size-1;
    
    while(true){
        if(start > end){
            break;
        }
        if(start==end){
            cout<<a[start]<<" ";
        }
        else{
            cout<<a[start]<<" ";
            cout<<a[end]<<" ";
        }
        start++;
         end--;
        
    }    
}
