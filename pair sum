#include <iostream>
#include<vector>

using namespace std;


int main()
{
    
vector<int>arr{10,20,30,40,50,60,70};

      
      
  
  for(int j=0;j<arr.size();j++){
     int  element=arr[j];
          
          
          for(int i=j+1;i<arr.size();i++){
              cout<<"("<<element<<","<<arr[i]<<")"<<endl;
          }
      }
  
 



}



