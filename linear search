#include <iostream>

using namespace std;

int main()
{   
    int a[]={10,12,13,2,1,5,4};
    int size=7;
    int key;
    bool flag=0;
    
    cout<<"enter element you want to search\n";
    cin>>key;
    for(int  i=0;i<size;i++){
        if(a[i]==key){
            flag=1;
            break;
        }
        
    }
    if(flag)
        cout<<"present";
    
    else
        cout<<"absent";
        
        
        
    
}
